        "svelte-preprocess": "^5.0.3",
        "tslib": "^2.4.1",
        "typescript": "^4.9.3",
        "vite": "^4.1.4",
        "vite-plugin-rsw": "^2.0.11"
    },
    "type": "module",
    "dependencies": {
        "cowsay2": "^2.0.4",
        "memfs": "^3.4.13"
